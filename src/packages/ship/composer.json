{
    "name": "ggphp/ship",
    "description": "A Package for write core boilerplate with jsonapi specification",
    "keywords": [
        "kun391",
        "ship"
    ],
    "homepage": "https://github.com/kun391/core",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Kun",
            "email": "nguyentruongthanh.dn@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^8.0",
        "prettus/l5-repository": "^2.6"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "laravel/telescope": "^4.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "autoload": {
        "psr-4": {
            "GGPHP\\Ship\\": "src/" 
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GGPHP\\Ship\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [],
            "providers": [
                "GGPHP\\Ship\\Providers\\ShipProvider"
            ],
            "aliases": {
                "Core": "GGPHP\\Ship\\ShipFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
